<div class="col-md-3 sidebar">
  <div class="row">
    <div class="col-md-12">
       <button class="btn btn-primary btn-block discord">Join Discord Server</button>
    </div>
    <div class="col-md-12">
      <iframe id="discord-widget" src="https://discordapp.com/widget?id=91981365452488704" frameborder="0" allowtransparency="true" frameborder="0"></iframe>
    </div>
  </div>
</div>
<div class="col-md-9 articals">
 <div class="row">
    <% for(var i = 0; i < data.news.length; i++) { %>
    <% if(data.news[i]) { %>
    <div class="col-md-12 artical">
      <h2><a href="<%- data.news[i].link %>"><%- data.news[i].title %></a><small> <%- data.news[i].author %></small></h2>
      <%- data.news[i].content %>
      <footer><%- data.news[i].published %></footer>
    </div>
    <% if(i+1 < data.news.length) { %>
    <div class="col-md-12"><hr></div>
    <% } %>
    <% } %>
    <% } %>
  </div>
</div> 